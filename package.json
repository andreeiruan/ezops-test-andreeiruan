    "mongoose": "^6.0.4",
    "pm2": "^5.1.1",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "husky": "^3.0.8",
    "jasmine": "^3.1.0",
    "nodemon": "^2.0.12",
    "request": "^2.87.0"
  }
}
