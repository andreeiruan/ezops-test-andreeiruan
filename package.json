{
  "name": "demo-chat-app",
  "version": "1.0.0",
  "description": "demo chat app",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-push": "eval $(docker-machine env ezops-test-andreeiruan) && docker kill $(docker ps -q) && docker image rmi simple-chat --force && docker build -t simple-chat . && docker run -p 3000:3000 -d simple-chat"
